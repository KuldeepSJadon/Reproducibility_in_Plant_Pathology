@article{Long_2015,
	Abstract = {Abstract To better understand the contribution of the quantitative fusarium head blight (FHB) resistance locus, 2DL \{QTL\} from Wuhan-1, we have characterized two pairs of sister lines carrying either the 2DL \{QTL\} or no known QTLs. In wheat spikes, our visual and microscopic observations suggested that the 2DL \{QTL\} suppress fungal infection in inoculated spikelets and reduce spread from spikelets to rachis. Transcriptome profiling of infected spikelets showed changes in expression for 487 probe sets between the 2DL-carrying and the null lines. Of 27 transcripts selected for validation, eight candidate genes with similar expression profiles between the two pair of lines were identified. },
	Author = {XiangYu Long and Margaret Balcerzak and Sigrun Gulden and Wenguang Cao and George Fedak and Yu-Ming Wei and You-Liang Zheng and Daryl Somers and Th{\'{e}}r{\`{e}}se Ouellet},
