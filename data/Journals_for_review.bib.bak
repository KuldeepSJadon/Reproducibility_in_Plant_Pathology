	Keywords = {Norway spruce},
	Month = {jan},
	Number = {1},
	Pages = {10--16},
	Publisher = {Elsevier {BV}},
	Title = {Defence-related gene expression in bark and sapwood of {Norway} spruce in response to \textit{Heterobasidion parviporum} and methyl jasmonate},
	Url = {http://www.sciencedirect.com/science/article/pii/S0885576511001147},
