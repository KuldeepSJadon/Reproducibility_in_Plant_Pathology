	Journaltitle = {Physiological and Molecular Plant Pathology},
	Keywords = {Plant development},
	Month = {jul},
	Pages = {89--96},
	Publisher = {Elsevier {BV}},
	Title = {Influence of leaf age on induced resistance in grapevine against \textit{Plasmopara viticola}},
	Url = {http://www.sciencedirect.com/science/article/pii/S0885576512000392},
	Volume = {79},
	Year = {2012},
	Bdsk-Url-1 = {http://www.sciencedirect.com/science/article/pii/S0885576512000392},
