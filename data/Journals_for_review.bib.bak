	Month = {jan},
	Number = {2},
	Pages = {127--132},
	Publisher = {Springer Nature},
	Title = {White clover mosaic virus-induced gene silencing in pea},
	Volume = {78},
	Year = {2012},
	Bdsk-Url-1 = {http://dx.doi.org/10.1007/s10327-012-0360-3}}

@article{Tashiro2012,
	Abstract = {In March 2006, stored fruits of the medium-to-late-ripening citrus variety Shiranuhi ([Citrus unshiu {\texttimes} C. sinensis] {\texttimes} C. reticulata) were found to have a disease similar to blue mold. The fungus causing this disease differed distinctly from the well-known, blue mold agent, Penicillium italicum, because it formed whisker-like coremia measuring 1--8 mm. On the basis of the morphological characteristics and the phylogenetic analysis based on the nucleotide sequence of the $\beta$-tubulin gene, the fungus was identified as P. ulaiense. This is the first report of citrus whisker mold caused by P. ulaiense in Japan.},
	Author = {Nobuya Tashiro and Kayo Manabe and Youichi Ide},
	Date = {2012-02},
	Date-Added = {2017-04-11 08:23:39 +0000},
