	Title = {Effects of nitrogen fertilization on insect pests, their parasitoids, plant diseases and volatile organic compounds in \textit{Brassica napus}},
	Url = {http://www.sciencedirect.com/science/article/pii/S0261219412002499},
	Volume = {43},
