	Issn = {0261-2194},
	Journal = {Crop Protection},
	Journaltitle = {Crop Protection},
	Keywords = {Solanum},
	Month = {jan},
	Pages = {21--27},
	Publisher = {Elsevier {BV}},
	Title = {Impact of quarantine procedures on weed biodiversity and abundance: Implications for the management of the golden potato cyst nematode, \textit{Globodera rostochiensis}},
	Url = {http://www.sciencedirect.com/science/article/pii/S0261219413002366},
	Volume = {55},
	Year = {2014},
