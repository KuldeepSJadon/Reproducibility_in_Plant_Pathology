	Journaltitle = {Journal of General Plant Pathology},
	Month = {feb},
	Number = {2},
	Pages = {140--144},
	Publisher = {Springer Nature},
	Title = {First report of whisker mold, a postharvest disease on citrus caused by \textit{Penicillium ulaiense} (in {Japan})},
	Volume = {78},
	Year = {2012},
	Bdsk-Url-1 = {http://dx.doi.org/10.1007/s10327-012-0363-0}}

